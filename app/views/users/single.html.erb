<div class="header">
  <div class="container">
    <img src="http://pix.iemoji.com/images/emoji/apple/ios-9/256/baby-bottle.png" width="90">
    <h1> <%= "Here's some stuff about " + @dude.name %> </h1>
    <% if current_guest %>
      </br>
      <h2> Signed in as <%= current_guest.guest_name %> </h2>
    <% end %>
  </div>
</div>


<div class="list">
  <h1>
      <p>
        <%= "Username:" + @dude.name %><br>  
        <%= "Found Following: " + @dude.following %><br>
        <%= "Phone Number: " + @dude.phone %><br> 
        <%= "Phone Carrier: " + @dude.carry %><br> 
        <%= "Phone Type: " + @dude.deviceType %><br> 
      </p>
  </h1>
</div>

<div class="list">
  <div class="button">
    <h1>
      <%= button_to "Back to Database", "/users/local", :method => "get" %>
      <%= button_to "Delete Item", users_killone_path(@dude), :method => "get" %>
    </h1>
  </div>
</div>